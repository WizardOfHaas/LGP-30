{
    "compilerOptions": {
      "lib": ["ES2022"],
      "isolatedModules": true,
      "esModuleInterop": true,
      "moduleResolution": "Bundler",
      "resolveJsonModule": true,
      "target": "ES2022",
      "strict": true,
      "allowJs": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "noEmit": true,
      "declaration": true,
      "module": "ES2022",
      "baseUrl": "src",
      "outDir": "lib",
      "strictPropertyInitialization": false,
      "noImplicitAny": false,
      "jsx": "react"
    }
}
  
